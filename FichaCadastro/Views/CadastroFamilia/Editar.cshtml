@using FichaCadastro.JSonNetContract
@using Newtonsoft.Json
@{
    ViewData["Title"] = "Editar Cadastro de Família";
}

<pre>
<code id="jsonDisplay">

    </code>

</pre>

@section scripts{
<script>
    $(document).ready(function(){
        @Html.Raw(" const familiaData = " + JsonConvert.SerializeObject(Model))

            $('#jsonDisplay').html(JSON.stringify(familiaData));
       
    });

</script>

}
